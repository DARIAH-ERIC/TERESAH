{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "laravel/framework": "4.2.11",
        "artdarek/oauth-4-laravel": "1.0.5",
        "barryvdh/laravel-debugbar": "1.7.7",
        "codesleeve/asset-pipeline": "dev-master",
        "easyrdf/easyrdf": "0.8.0",
        "graham-campbell/markdown": "2.0.5",
        "ml/json-ld": "1.0.3",
        "twitter/typeahead.js": "0.10.5",
        "watson/active": "1.2.4",
        "watson/validating": "0.10.4",
        "fabpot/goutte": "v3.2.1"
    },
    "require-dev": {
        "mockery/mockery": "0.9.2",
        "phpunit/phpunit": "4.3.3"
    },
    "autoload": {
        "classmap": [
            "app/commands",
            "app/controllers",
            "app/helpers",
            "app/libraries",
            "app/models",
            "app/observers",
            "app/repositories",
            "app/services",
            "app/database/migrations",
            "app/database/seeds",
            "app/tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable"
}
